.jumbotron
    h4  Leaderboard
    table.table.table-striped
        tr
            th Leader
            th Programs
            th Points
        tr(ng-repeat="(name, programs) in leaders.leaders")
            td {{name}}
            td
                ul
                    li.program._name(
                        ng-repeat="filename in programs.programs"
                    )
                        | {{filename}}
            td {{ programs.points }}
